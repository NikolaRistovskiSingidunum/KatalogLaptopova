# Aktivacija modula
RewriteEngine On

# Base path putanja /nedelja01/ stoji kod njega 
RewriteBase /KatalogLaptopova/KatalogLaptopova/nedelja01/

# Ako nije file, skip
RewriteCond %{REQUEST_FILENAME} !-f

# Ako nije dir, skip
RewriteCond %{REQUEST_FILENAME} !-d

# Sve preusmeri na...
RewriteRule ^(.*)$ ./index.php?URL=$1 [L]
